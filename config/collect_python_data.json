{
  "sources": [
    {
      "type": "github",
      "name": "python_tutorials",
      "repo": "python/cpython",
      "path": "Doc/tutorial",
      "token": "your_github_token"
    },
    {
      "type": "github",
      "name": "python_examples",
      "repo": "pytorch/examples",
      "path": "",
      "token": "your_github_token"
    },
    {
      "type": "api",
      "name": "stackoverflow_python",
      "url": "https://api.stackexchange.com/2.3/questions",
      "params": {
        "site": "stackoverflow",
        "tagged": "python",
        "sort": "votes",
        "order": "desc",
        "filter": "withbody"
      },
      "items_key": "items",
      "has_more_key": "has_more",
      "delay": 2
    },
    {
      "type": "web",
      "name": "python_docs",
      "urls": [
        "https://docs.python.org/3/tutorial/index.html",
        "https://docs.python.org/3/tutorial/appetite.html",
        "https://docs.python.org/3/tutorial/interpreter.html"
      ],
      "selectors": {
        "container": "div.section",
        "title_text": "h1",
        "content_html": "div.body"
      }
    },
    {
      "type": "file",
      "name": "local_examples",
      "path": "./existing_data/python_examples.json"
    }
  ],
  "output_format": "json"
}
